def fib(n):
    if n < 2:
        return n
    else:
        return fib(n - 2) + fib(n - 1)

def countfib(n):
    bef = (new Date).getTime()/1000
    for i in range(n):
        print(fib(i))
    aft = (new Date).getTime()/1000
    print('Duration: ' + (aft - bef) + ' seconds')

#countfib(20)

module.exports = {
    fib: fib,
    countfib: countfib
}